{
  "lang": "en",
  "fileExtensions": [
    ".md",
    ".texy"
  ],
  "ignoreUrls": true,
  "ignoreCapitalization": true,
  "ignoreText": [
    ["/--[php|html|neon]?[^]*?\\\\--", "gi"],
    ["[a-zA-Z0-9]+\\(.+\\)", "gi"],
    ["\\[.*\\|?.*\\]", "gi"],
    ["`.+`", "gi"],
    ["[0-9]+[a-z]+", "gi"],
    ["[a-z]{1,2}[0-9]", "gi"],
    ["/-+div", "gi"]
  ],
  "dictionary": [
    "nette",
    "(g|G)it(g|H)ub",
    "perex",
    "newline",
    "latte",
    "whitespace",
    "prepends",
    "regex",
    "subrequest",
    "utils",
    "timezone",
    "packagist",
    "classmap",
    "recursing",
    "runnable",
    "svn",
    "autoload[ed]",
    "PSR",
    "int",
    "componentmodel",
    "filesystem",
    "DI",
    "Tracy",
    "RobotLoader",
    "cachestorage",
    "(SQLite|sqlite|Sqlite)",
    "oci",
    "(SQLSRV|sqlsrv)",
    "DBLIB",
    "mssql",
    "(PDO|pdo)",
    "DSN",
    "snippetArea",
    "JsonResponse",
    "RegexpException",
    "InvalidArgumentException"
  ]
}
